\frametitle{Triggers}
Trigger 5:
\begin{tiny}
\begin{verbatim}
CREATE OR REPLACE TRIGGER VALID_DURATION
    BEFORE INSERT ON MOVIEINDEX.MOVIE
    FOR EACH ROW MODE DB2SQL
    WHEN (MOVIE.DURATION <= 0)
    BEGIN ATOMIC
        SIGNAL SQLSTATE '75001' ('DURATION MUST BE MORE THAN 0')
    END;
\end{verbatim}
\end{tiny}
