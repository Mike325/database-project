{
    "project.vim": {
        "type": "Project"
    },
    "UltiSnips/*.snippets": {
        "type": "UltiSnips"
    },
    ".ycm_extra_conf.py": {
        "type": "YCM"
    },
    ".projections.json": {
        "type": "Projections"
    },
    ".gitignore": {
        "type": "Gitignore"
    },
    ".git/hooks/*": {
        "type": "GitHooks"
    },
    ".git/config": {
        "type": "Git"
    },
    ".git/info/*": {
        "type": "Git"
    },
    "README.md": {
        "type": "Readme"
    },
    "LICENSE": {
        "type": "License"
    },
    "clang-format": {
        "type": "ClangFormat"
    },
    "*.sql": {
        "type": "DB2"
    },
    "tables.sql": {
        "type": "Tables"
    },
    "data.sql": {
        "type": "Data"
    },
    "views.sql": {
        "type": "View"
    }
}
